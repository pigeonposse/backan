{
	"name": "create-backan",
	"version": "0.0.15",
	"type": "module",
	"license": "GPL-3.0",
	"homepage": "https://backan.pigeonposse.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/pigeonposse/backan",
		"directory": "packages/create"
	},
	"bugs": {
		"url": "https://github.com/pigeonposse/backan/issues"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"bin": "dist/bin.js",
	"files": [
		"dist",
		"templates"
	],
	"module": "dist/main.js",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"exports": {
		".": "./dist/main.js",
		"./types": "./dist/main.d.ts"
	},
	"keywords": [
		"backend",
		"api",
		"rest-api",
		"openapi",
		"library",
		"bin",
		"create",
		"creator"
	],
	"scripts": {
		"dev": "tsx src/bin.ts",
		"example": "tsx examples/main.ts",
		"build": "pnpm run --sequential \"/^build:.*/\"",
		"build:rm": "rimraf dist && rimraf build",
		"build:lib": "vite build",
		"test": "pnpm run --sequential \"/^test:.*/\"",
		"test:unit": "vitest run src --passWithNoTests",
		"lint": "pnpm eslint src"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"picocolors": "^1.0.1"
	}
}
