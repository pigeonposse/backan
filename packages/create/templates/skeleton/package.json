{
	"name": "{{name}}",
	"version": "0.0.1",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"dev-server": "tsx src/server.ts",
		"build": "vite build",
		"build-bin": "backan-builder --input=dist/server.js",
		"test": "pnpm run --sequential \"/^test:.*/\"",
		"test:unit": "vitest run src --passWithNoTests",
		"test:e2e": "playwright test",
		"lint": "pnpm eslint src"
	},
	"devDependencies": {
		"@backan/builder": "{{backan_version}}",
		"@backan/server": "{{backan_version}}",
		"eslint": "^9.9.0",
		"globals": "^15.9.0",
		"typescript-eslint": "^8.1.0",
		"@playwright/test": "1.46.1",
		"typescript": "5.5.4",
		"vite": "5.4.1",
		"tsx": "^4.17.0",
		"vite-plugin-dts": "4.0.3",
		"vitest": "2.0.5"
	},
	"dependencies": {
		"@backan/core": "{{backan_version}}"
	}
}
