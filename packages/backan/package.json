{
	"name": "backan",
	"version": "0.2.2",
	"description": "All in one web API builder. Create endpoints with type validations and OpenApi documentation, safely and quickly.",
	"keywords": [
		"api",
		"backend",
		"js",
		"library",
		"node",
		"openapi",
		"pigeonposse",
		"pp",
		"rest-api"
	],
	"homepage": "https://backan.pigeonposse.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/pigeonposse/backan"
	},
	"license": "GPL-3.0",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/main.d.ts",
			"import": "./dist/main.js"
		},
		"./stream": {
			"types": "./dist/stream.d.ts",
			"import": "./dist/stream.js"
		}
	},
	"main": "dist/main.js",
	"module": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm run --sequential \"/^build:.*/\"",
		"build:_rm": "rimraf dist && rimraf build",
		"build:lib": "vite build",
		"dev": "vite dev"
	},
	"dependencies": {
		"@backan/core": "workspace:*"
	},
	"devDependencies": {
		"@hono/vite-dev-server": "0.18.1"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
